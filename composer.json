{
    "name"        : "scribe/swim-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/swim-bundle",
    "description" : "Simple and extensible parser library for markdown, Swim, and more...",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://ScribeCacheBundle.docs.scribe.tools/issues",
        "source" : "https://ScribeCacheBundle.docs.scribe.tools/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "scribe-cache-bundle",
        "symfony-cache-bundle",
        "mantle-cache",
        "cache",
        "bundle",
        "symfony",
        "symfony2",
        "symfony-framework",
        "scribe",
        "scr",
        "scribenet",
        "scribeinc",
        "scribe-software",
        "scribe-services"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        },
        {
            "name"    : "Dan Corrigan",
            "email"   : "dfc@scr.be",
            "homepage": "https://scr.be/dfc",
            "role"    : "Developer (Rogue Rubyist)"
        }
    ],
    "require" : {
        "php"                                  : ">=5.6.4",
        "scribe/mantle-bundle"                 : "~2.0|~2.1@dev",
        "scribe/cache-bundle"                  : "~2.0|~2.1@dev",
        "symfony/console"                      : "~2.7|~2.8@dev",
        "symfony/dependency-injection"         : "~2.7|~2.8@dev",
        "symfony/twig-bundle"                  : "~2.7|~2.8@dev",
        "symfony/twig-bridge"                  : "~2.7|~2.8@dev",
        "twig/twig"                            : "~1.18",
        "erusev/parsedown"                     : "~1.5",
        "erusev/parsedown-extra"               : "~0.7"
    },
    "require-dev" : {
        "ext-memcached"                        : "~2.2",
        "ext-igbinary"                         : "~1.2",
        "ext-tidy"                             : "~2.0",
        "symfony/symfony"                      : "~2.7|~2.8@dev",
        "symfony/stopwatch"                    : "~2.7|~2.8@dev",
        "symfony/framework-bundle"             : "~2.7|~2.8@dev",
        "symfony/phpunit-bridge"               : "~2.7|~2.8@dev",
        "phpunit/phpunit"                      : "~4.6",
        "sensio/generator-bundle"              : "~2.5",
        "doctrine/orm"                         : "~2.5",
        "doctrine/common"                      : "~2.5",
        "doctrine/dbal"                        : "~2.5",
        "doctrine/doctrine-bundle"             : "~1.4",
        "doctrine/doctrine-fixtures-bundle"    : "2.2.*@dev",
        "doctrine/data-fixtures"               : "1.0.*@dev",
        "sensiolabs/security-checker"          : "~2.0",
        "scrutinizer/ocular"                   : "~1.1",
        "satooshi/php-coveralls"               : "*",
        "chriskite/phactory"                   : "~0.4",
        "sami/sami"                            : "~3.0",
        "nikic/php-parser"                     : ">=1.0,<1.2"
    },
    "autoload": {
        "psr-4": {
            "Scribe\\SwimBundle\\": "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
