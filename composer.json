{
    "name"        : "scribe/swim-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://cache-bundle.oss.scr.be/home",
    "description" : "Simple and extensible parser library for markdown, Swim, and more...",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://cache-bundle.oss.scr.be/issues",
        "source" : "https://cache-bundle.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "swim-bundle", "scribe-swim-bundle", "swim", "markdown", "symfony", "scribe", "scribenet"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        },
        {
            "name"    : "Dan Corrigan",
            "email"   : "dfc@scr.be",
            "homepage": "https://scr.be/dfc",
            "role"    : "Developer (Rogue Rubyist)"
        }
    ],
    "require" : {
        "php"                          : ">=5.6.4,~5.6",
        "ext-memcached"                : "~2.2",
        "scribe/wonka-library"         : "~0.1@dev",
        "scribe/wonka-bundle"          : "~0.1@dev",
        "scribe/cache-bundle"          : "~2.1@dev",
        "symfony/dependency-injection" : "~2.7|~2.8@dev",
        "erusev/parsedown"             : "~1.5",
        "erusev/parsedown-extra"       : "~0.7"
    },
    "require-dev" : {
        "ext-igbinary"                : "~1.2",
        "symfony/symfony"             : "~2.7|~2.8@dev",
        "symfony/framework-bundle"    : "~2.7|~2.8@dev",
        "symfony/monolog-bundle"      : "~2.7|~2.8@dev",
        "symfony/stopwatch"           : "~2.7|~2.8@dev",
        "symfony/var-dumper"          : "~2.7|~2.8@dev",
        "symfony/debug-bundle"        : "~2.7|~2.8@dev",
        "symfony/phpunit-bridge"      : "~2.7|~2.8@dev",
        "phpunit/phpunit"             : "~4.8",
        "sensiolabs/security-checker" : "~3.0",
        "scrutinizer/ocular"          : "~1.1",
        "satooshi/php-coveralls"      : "~0.6",
        "chriskite/phactory"          : "~0.4",
        "fzaninotto/faker"            : "~1.5",
        "sami/sami"                   : "~3.0",
        "nikic/php-parser"            : ">=1.0,<1.2"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.1"
    },
    "autoload": {
        "psr-4": {
            "Scribe\\SwimBundle\\": "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
