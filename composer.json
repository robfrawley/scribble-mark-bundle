{
    "name": "scribe/swim-bundle",
    "description": "Simple and extendable parser library for markdown, Swim, and more...",
    "keywords": ["Symfony2", "Swim", "Scribe"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/scribenet/ScribeSwimBundle",
    "license": "MIT",
    "authors": [{
        "name" : "Rob Frawley 2nd",
        "email" : "rfrawley@scribenet.com",
        "homepage": "http://www.scribenet.com"
    }],
    "require": {
        "php"                      : ">=5.6",
        "symfony/framework-bundle" : ">=2.0",
        "symfony/monolog-bridge"   : ">=2.0",
        "scribe/cache-bundle"      : "2.0.*@dev",
        "scribe/mantle-bundle"     : "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {"Scribe\\SwimBundle\\": "src/"}
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
